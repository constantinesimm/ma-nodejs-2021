{
  "restartable": "rs",
  "ignore": [
    ".git",
    ".idea",
    "uploads",
    "node_modules/**/node_modules"
  ],
  "verbose": true,
  "execMap": {
    "js": "node --harmony"
  },
  "events": {
    "restart": "Application restarted due to change files --> nodemon"
  },
  "watch": [
    ".bin/www",
    "src",
    "config"
  ],
  "env": {
    "NODE_ENV": "development"
  },
  "ext": "js,json"
}
