{
  "env": {
    "node": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": [
    "airbnb-base",
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": 2021
  },
  "rules": {
    "quotes": [
      "error",
      "single"
    ],
    "no-console": "off",
    "max-len": [
      "warn",
      {
        "code": 125
      }
    ],
    "semi": "warn",
    "no-restricted-syntax": ["error", "FunctionExpression", "WithStatement"]
  }
}
